<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="container" prefHeight="478.0" prefWidth="910.0" spacing="8.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.forms.search_row.SearchRow">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="8.0" VBox.vgrow="NEVER">
         <children>
            <Label text="Search:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label fx:id="labelTableName" layoutX="10.0" layoutY="10.0" text="Search Row">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="textFieldSearch" promptText="search..." HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <TableView fx:id="tableViewRows" VBox.vgrow="ALWAYS" />
      <Label fx:id="labelStatus" text="status" />
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
